{
  "name": "globomantics",
  "buildCommand": "cd globomantics.main1 && npm run build",
  "outputDirectory": "globomantics.main1/.next",
  "installCommand": "cd globomantics.main1 && npm install",
  "functions": {
    "globomantics.main1/pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/globomantics.main1/pages/api/$1"
    }
  ],
  "regions": ["iad1"]
}
